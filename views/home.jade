extends layout

block content
    .container-fluid
        table.table.table-hover
            tbody
                for article in articles
                    tr
                        if !article.title
                            td #{article.story.story_title}
                        else
                            td #{article.title}
                        td #{article.date_created_friendly}
                        td
                            a(href='#')
                                i.fa.fa-trash


